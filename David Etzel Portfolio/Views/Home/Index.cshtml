            $("#palOut").text("The reverse of \"" + word + "\" is \"" + reverse + "\".");
            $("#palResult").text("\"" + word + "\" is not a palindrome.");
        } else {
            $("#palOut").text("\"" + word + "\" backwards is \"" + word + "\".");
            $("#palResult").text("\"" + word + "\" is a palindrome.");

                                        <input id="five1" class="input form-control" placeholder="Enter Number 1..." />
                                        <input id="five2" class="input form-control" placeholder="Enter Number 2..." />
                                        <input id="five3" class="input form-control" placeholder="Enter Number 3..." />
                                        <input id="five4" class="input form-control" placeholder="Enter Number 4..." />
                                        <input id="five5" class="input form-control" placeholder="Enter Number 5..." />
                                        <br />
                                    </div>
                                </div>
                                <div class="col-md-offset-1 col-md-5">
                                    <div class="row text-center">
                                        <h5>Results:</h5>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-offset-2 col-md-8">
                                            <h5 id="smallNum" class="output"></h5>
                                            <h5 id="largeNum" class="output"></h5>
                                            <h5 id="avgNum" class="output"></h5>
                                            <h5 id="sumNum" class="output"></h5>
                                            <h5 id="prodNum" class="output"></h5>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="modal-footer">
                                <button id="fiveCalc" class="btn btn-success">Calculate</button>
                                <button id="fiveClear" class="btn btn-success">Clear</button>
                                <button id="toggle1" class="btn btn-success">Show Code</button>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div id="code1">
                                    <pre class="brush: js">
$(function () {
    $("#code1").hide();

    //Calculations for Five Numbers
    $("#fiveCalc").click(function () {
        var a, b, c, d, e;
        a = +$("#five1").val();
        b = +$("#five2").val();
        c = +$("#five3").val();
        d = +$("#five4").val();
        e = +$("#five5").val();
        var small = Math.min(a, b, c, d, e);
        $("#smallNum").text("SMALLEST = " + small);
        var big = Math.max(a, b, c, d, e);
        $("#largeNum").text("GREATEST = " + big);
        var avg = (a + b + c + d + e) / 2;
        $("#avgNum").text("AVERAGE = " + avg);
        var sum = a + b + c + d + e;
        $("#sumNum").text("SUM = " + sum);
        var prod = a * b * c * d * e;
        $("#prodNum").text("PRODUCT = " + prod);
    });

    //Clearing all Five Number Input and Output Fields
    $("#fiveClear").click(function () {
        $(".input").val("");
        $(".output").text("");
    });

    //Five Number "Show/Hide Code" Button Operations
    $("#toggle1").click(function () {
        $("#code1").toggle();
        if ($(this).text() == "Show Code") {
            $(this).text("Hide Code");
        } else {
            $(this).text("Show Code");
        }
    });
});
